<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="corePane" prefHeight="527.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.eduard.musicplayer.ErrorHandler">
   <children>
      <Label layoutX="14.0" layoutY="39.0" text="Something did not quite work... :(">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="73.0" text="A problem has occurred while trying to make this action. Important error logs are shown below. Please report to the developer.">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="errorLog" alignment="TOP_LEFT" layoutX="14.0" layoutY="99.0" prefHeight="354.0" prefWidth="861.0" text="[Error Log Placeholder]" wrapText="true">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Button fx:id="okButton" layoutX="786.0" layoutY="488.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="OK" />
      <Rectangle fx:id="taskBar" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="30.0" layoutY="-1.0" stroke="BLACK" strokeType="INSIDE" width="880.0" />
      <ImageView fitHeight="25.0" fitWidth="32.0" layoutX="844.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true" fx:id="xImage" />
      <ImageView fx:id="minimizeImage" fitHeight="25.0" fitWidth="32.0" layoutX="804.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="840.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="40.0" fx:id="xButton" />
      <Button fx:id="minimizeButton" layoutX="800.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="40.0" />
      <Label fx:id="taskbarTitle" layoutX="9.0" layoutY="5.0" text="Window Title" />
      <Button fx:id="copyDescription" layoutX="616.0" layoutY="488.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="164.0" text="Copy Problem Description" />
      <Button fx:id="copyFullTrace" layoutX="465.0" layoutY="488.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="144.0" text="Copy Full Stack Trace" />
      <Button fx:id="copyImportantTrace" layoutX="294.0" layoutY="488.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="164.0" text="Copy Important Stack Trace" />
      <Label fx:id="statusLabel" alignment="CENTER" layoutX="361.0" layoutY="46.0" prefHeight="17.0" prefWidth="510.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="453.0" text="Problem still persisting? Try out the playlist fix feature under the &quot;Manage Playlist&quot; option in the &quot;Options...&quot; menu" />
   </children>
</AnchorPane>
