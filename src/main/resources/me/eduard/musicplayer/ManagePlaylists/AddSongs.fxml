<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="corePane" prefHeight="506.0" prefWidth="826.0" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="me.eduard.musicplayer.Components.ManagePlaylist.AddSongs">
    <Label layoutX="12.0" layoutY="40.0"
           text="Input all youtube links to download the songs. Separate each link by &quot;,&quot;">
        <font>
          <Font size="24.0"/>
        </font>
    </Label>
    <TextArea fx:id="textArea" layoutX="9.0" layoutY="81.0" prefHeight="347.0" prefWidth="812.0"/>
    <Button fx:id="proceedButton" layoutX="724.0" layoutY="465.0" mnemonicParsing="false" text="Update Playlist"/>
    <Button fx:id="cancelButton" layoutX="618.0" layoutY="465.0" mnemonicParsing="false" prefHeight="25.0"
            prefWidth="95.0" text="Cancel"/>
    <Label fx:id="statusLabel" alignment="CENTER" layoutX="12.0" layoutY="464.0" prefHeight="28.0" prefWidth="479.0"
           textFill="RED">
        <font>
          <Font size="19.0"/>
        </font>
    </Label>
    <Rectangle fx:id="taskBar" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="30.0" stroke="BLACK"
               strokeType="INSIDE" width="826.0"/>
    <ImageView fitHeight="26.0" fitWidth="32.0" layoutX="790.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true"
               fx:id="xImage"/>
    <ImageView fx:id="minimizeImage" fitHeight="26.0" fitWidth="32.0" layoutX="750.0" layoutY="1.0" pickOnBounds="true"
               preserveRatio="true"/>
    <Button layoutX="786.0" layoutY="1.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="40.0" fx:id="xButton"/>
    <Button fx:id="minimizeButton" layoutX="746.0" layoutY="1.0" mnemonicParsing="false" prefHeight="28.0"
            prefWidth="40.0"/>
    <Label fx:id="taskbarTitle" layoutX="8.0" layoutY="6.0" text="Window Title"/>
    <MenuBar layoutX="497.0" layoutY="465.0">
        <Menu mnemonicParsing="false" text="Video Quality...">
            <MenuItem fx:id="quality6" mnemonicParsing="false" text="Highest Possible"/>
            <MenuItem fx:id="quality5" mnemonicParsing="false" text="1080p - High"/>
            <MenuItem fx:id="quality4" mnemonicParsing="false" text="720p - Above-Medium"/>
            <MenuItem fx:id="quality3" mnemonicParsing="false" text="480p - Medium"/>
            <MenuItem fx:id="quality2" mnemonicParsing="false" text="360p - Low-Medium"/>
            <MenuItem fx:id="quality1" mnemonicParsing="false" text="240p - Low"/>
            <MenuItem fx:id="quality0" mnemonicParsing="false" text="144p - Lowest"/>
        </Menu>
    </MenuBar>
    <Label fx:id="qualityLabel" alignment="CENTER" layoutX="9.0" layoutY="435.0" prefHeight="26.0" prefWidth="812.0"
           text="Getting default video quality">
        <font>
          <Font size="17.0"/>
        </font>
    </Label>
</AnchorPane>
