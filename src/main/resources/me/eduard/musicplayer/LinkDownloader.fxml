<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="corePane" prefHeight="540.0" prefWidth="887.0" xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.eduard.musicplayer.Components.LinkDownloader">
   <TextArea fx:id="linkArea" layoutX="10.0" layoutY="109.0" prefHeight="347.0" prefWidth="868.0">
      <font>
         <Font size="14.0"/>
      </font>
   </TextArea>
   <Label fx:id="titleLabel" layoutX="10.0" layoutY="40.0"
          text="Input all youtube links to download the songs. Separate each link by &quot;,&quot;">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <Button fx:id="createButton" layoutX="768.0" layoutY="501.0" mnemonicParsing="false" prefHeight="25.0"
           prefWidth="108.0" text="Create Playlist"/>
   <Button fx:id="cancelButton" layoutX="653.0" layoutY="501.0" mnemonicParsing="false" prefHeight="25.0"
           prefWidth="108.0" text="Cancel"/>
   <Label fx:id="playListNameLabel" layoutX="14.0" layoutY="75.0" text="Playlist Name">
      <font>
         <Font size="17.0"/>
      </font>
   </Label>
   <TextField fx:id="playlistName" layoutX="124.0" layoutY="75.0" prefHeight="25.0" prefWidth="752.0"/>
   <Label fx:id="statusLabel" alignment="CENTER" layoutX="11.0" layoutY="501.0" prefHeight="25.0" prefWidth="514.0"
          textFill="#0073ff">
      <font>
         <Font size="16.0"/>
      </font>
   </Label>
   <Rectangle fx:id="taskBar" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="30.0" stroke="BLACK"
              strokeType="INSIDE" width="887.0"/>
   <ImageView fitHeight="26.0" fitWidth="32.0" layoutX="851.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true"
              fx:id="xImage"/>
   <ImageView fx:id="minimizeImage" fitHeight="26.0" fitWidth="32.0" layoutX="811.0" layoutY="2.0" pickOnBounds="true"
              preserveRatio="true"/>
   <Button layoutX="847.0" layoutY="2.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="40.0" fx:id="xButton"/>
   <Button fx:id="minimizeButton" layoutX="807.0" layoutY="2.0" mnemonicParsing="false" prefHeight="28.0"
           prefWidth="40.0"/>
   <Label fx:id="taskbarTitle" layoutX="8.0" layoutY="7.0" text="Window Title"/>
   <MenuBar layoutX="531.0" layoutY="501.0">
      <Menu mnemonicParsing="false" text="Video Quality...">
         <MenuItem fx:id="quality6" mnemonicParsing="false" text="Highest Possible"/>
         <MenuItem fx:id="quality5" mnemonicParsing="false" text="1080p - High"/>
         <MenuItem fx:id="quality4" mnemonicParsing="false" text="720p - Above-Medium"/>
         <MenuItem fx:id="quality3" mnemonicParsing="false" text="480p - Medium"/>
         <MenuItem fx:id="quality2" mnemonicParsing="false" text="360p - Low-Medium"/>
         <MenuItem fx:id="quality1" mnemonicParsing="false" text="240p - Low"/>
         <MenuItem fx:id="quality0" mnemonicParsing="false" text="144p - Lowest"/>
      </Menu>
   </MenuBar>
   <Label fx:id="qualityLabel" alignment="CENTER" layoutX="14.0" layoutY="466.0" prefHeight="26.0" prefWidth="868.0"
          text="Getting default video quality">
      <font>
         <Font size="17.0"/>
      </font>
   </Label>
</AnchorPane>
